@model MvcLibrary.Models.Checkout

@{
    ViewData["Title"] = "Encheckout";
}

<h1>End checkout</h1>

<h2>Do you want to end this checkout by "@Html.DisplayFor(model => model.UserName)"?</h2>

<hr />

<form asp-asp-controller="Checkouts" asp-action="EndCheckout">
    <input type="hidden" asp-for="Id" />
    <input type="submit" value="End checkout" class="btn btn-danger" />
    <a asp-area="" asp-controller="Books" asp-action="Index">Back to List</a>
</form>

